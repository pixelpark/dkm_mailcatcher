<% | String $description,
     String $documentation,
     String $after,
     String $type,
     String $execstart,
     String $restart,
     String $wantedby,
     String $restart_time,
| -%>

[Unit]
Description=<%= $description %>
Documentation=<%= $documentation %>
After=<%= $after %>

[Service]
Type=<%= $type %>
ExecStart=<%= $execstart %>
Restart=<%= $restart %>
RestartSec=<%= $restart_time %>

[Install]
WantedBy=<%= $wantedby %>
